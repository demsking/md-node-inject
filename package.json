  },
  "homepage": "https://github.com/demsking/unist-util-inject#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "standard": "^10.0.3"
  },
